; A ~= X*Y/256
mult
    lda log2tab,x           ; get log
    sta getresult+1         ; store log (self-modifying code)
    ldx log2tab,y           ; get log
getresult
    lda pow2tab,x           ; get result (self-modified code)
    rts                     ;
